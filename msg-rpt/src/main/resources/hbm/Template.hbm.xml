<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping default-access="field">
    <class name="com.wjs.msg.domain.template.Template" table="TB_TEMPLATE" batch-size="100" schema="msg_database" where="logic_delete = 0">
        <!--<cache usage="read-only" />-->
        <comment>消息模板</comment>
        <id name="id" access="property">
            <column name="id" not-null="true" length="20">
                <comment>主键</comment>
            </column>
            <generator class="identity"/>
        </id>
        <version name="version" type="long">
            <column name="version" default="0" not-null="true">
                <comment>版本号</comment>
            </column>
        </version>
        <property name="createTime" type="timestamp" update="false">
            <column name="create_time" not-null="true" default="current_timestamp" length="50">
                <comment>创建时间</comment>
            </column>
        </property>
        <property name="updateTime" type="timestamp">
            <column name="update_time" not-null="true" default="current_timestamp" length="50">
                <comment>更新时间</comment>
            </column>
        </property>
        <property name="logicDelete">
            <column name="logic_delete" not-null="true" default="0">
                <comment>是否已逻辑删除</comment>
            </column>
        </property>
        <property name="context">
            <column name="context">
                <comment>内容</comment>
            </column>
        </property>
        <many-to-one name="group">
            <column name="group_id">
                <comment>消息组外键</comment>
            </column>
        </many-to-one>
        <many-to-one name="router">
            <column name="router_id">
                <comment>路由外键</comment>
            </column>
        </many-to-one>
        <property name="protocol">
            <column name="protocol">
                <comment>协议</comment>
            </column>
        </property>
    </class>
</hibernate-mapping>